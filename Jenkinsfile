pipeline {
    agent any
    tools {
        jdk 'jdk-14'
    }
    stages {
        stage("build project") {
            steps {
                sh 'java -version'
                echo 'building project...'
                sh 'ls'
                sh 'chmod +x android/gradlew'
                sh 'keytool -genkeypair -v -storetype PKCS12 -keystore asosoft.keystore -alias asosoft -keyalg RSA -keysize 2048 -validity 10000'
                sh 'asosoft123'
                sh 'asosoft123'
                sh 'asosoft'
                sh 'asosoft'
                sh 'asosoft'
                sh 'Guatemala'
                sh 'Guatemala'
                sh 'GT'
                sh 'yes'
                sh 'android/gradlew tasks'
                sh 'android/gradlew clean'
                sh 'android/gradlew assembleRelease'
            }
        }
    }
}